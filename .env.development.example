# Конфигурация Сервера
NODE_ENV=development
LISTEN_HOST=localhost
LISTEN_PORT=8080
BASE_PATH=
LOG_LEVEL=debug
TZ=UTC

# Лимиты Обработки
FILE_MAX_BYTES_MB=25 # Максимальный размер файла (25MB)
MAX_CONCURRENCY=2 # Количество одновременных задач (рекомендуется = кол-во ядер CPU)
QUEUE_TIMEOUT_SECONDS=30 # Время ожидания задачи в очереди перед сбросом (30 сек)
REQUEST_TIMEOUT_SECONDS=60 # Общий таймаут запроса вместе с очередью (60 сек)

# Shutdown
SHUTDOWN_TIMEOUT_SECONDS=30 # Время на завершение активных задач при остановке (30 сек)